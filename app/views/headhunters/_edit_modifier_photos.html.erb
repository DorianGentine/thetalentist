<!-- modifier photos -->

<%= simple_form_for @startup, url: update_photos_headhunter_path(@headhunter), method: :patch, remote: false  do |f| %>
  <%= f.simple_fields_for :pictures do |u| %>
    <%= u.input :photo %>
    <%= u.input :photo_cache, as: :hidden %>
  <% end %>
  <%= f.submit %>
<% end %>
